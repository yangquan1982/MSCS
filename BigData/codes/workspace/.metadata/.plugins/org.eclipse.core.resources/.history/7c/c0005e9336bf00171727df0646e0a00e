package lab3.q1;

public class WordCount {
	private int m;
	private int r;
	private Mapper[] mapper;
	private Reducer[] reducer;
	public WordCount(int m, int r) {
		this.m = m;
		this.r = r;
		mapper = new Mapper[m];
		reducer = new Reducer[r];
	}
	public int getPartition(String key){
		return (int) key.hashCode() % r;
	}
}
